{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.OpenTelemetry"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "OpenTelemetry",
        "Args": {
          "serverUrl": "http://localhost:4319"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Trench.Notification.Api",
        "Uri": "https://localhost:7068/_healthcheck"
      }
    ]
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "SupportedCultures": {
    "Cultures": {
      "en-US": "en-US",
      "pt-BR": "pt-BR"
    }
  }
}